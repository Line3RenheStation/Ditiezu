<?xml version="1.0" encoding="utf-8"?>
<resources>

    ima<style name="Theme.Ditiezu" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorAccent">@color/primary_default</item>
        <item name="colorPrimary">@color/primary_default</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/primary_default</item>
        <item name="colorSecondaryVariant">@color/primary_default</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="Theme.Ditiezu.TabLayout" parent="Theme.Ditiezu">
        <item name="tabBackground">@android:color/transparent</item>
        <item name="tabIndicatorColor">@color/primary_default</item>
        <item name="tabIndicatorHeight">2dp</item>
        <item name="tabTextAppearance">@style/TextAppearance.Design.Tab</item>
        <item name="tabSelectedTextColor">@color/primary_light</item>
    </style>

    <style name="Theme.Ditiezu.AppBarOverlay">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Ditiezu.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.null.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>